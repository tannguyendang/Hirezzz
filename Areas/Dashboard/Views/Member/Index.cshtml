@model List<Member>
<table class="table table-bordered">
    <thead class="table-dark">
        <tr>
            <th>Id</th>
            <th>Username</th>
            <th>Email</th>
            <th>Fullname</th>
            <th>Gender</th>
            <th>Roles</th>
            <th>Delete</th>
            <th>Edit</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Username</td>
                <td>@item.Email</td>
                <td>@item.Fullname</td>
                <td>@item.Gender</td>
                <td>
                    <a href="/dashboard/member/roles/@item.Id">Roles</a>
                </td>
                <td>
                    <a href="/dashboard/member/delete/@item.Id"><i class="fa-sharp fa fa-trash"></i></a>
                </td>
                <td>
                    <a href="/dashboard/member/edit/@item.Id"><i class="fa-sharp fa fa-wrench"></i></a>
                </td>
            </tr>
        }
    </tbody>
</table>